{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js --env.mode=dev --progress",
    "build": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path /fis-live/dist/ --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path / --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^7.2.6",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@angular/forms": "^7.2.6",
    "@angular/platform-browser": "^7.2.6",
    "@angular/platform-browser-dynamic": "^7.2.6",
    "@angular/router": "^7.2.6",
    "@clr/icons": "^1.1.0",
    "@clr/ui": "^1.1.0",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/entity": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngrx/store-devtools": "^7.2.0",
    "core-js": "^2.6.5",
    "ngrx-store-localstorage": "^7.0.0",
    "perfect-scrollbar": "^1.4.0",
    "rxjs": "^6.4.0",
    "semantic-ui-css": "^2.4.1",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.13.3",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/language-service": "^7.2.6",
    "@angular/platform-server": "^7.2.6",
    "@ngtools/webpack": "^7.3.3",
    "@types/node": "^10.12.26",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^9.4.8",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^4.5.0",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.10",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.0.6",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.1",
    "typescript": "~3.2.4",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  }
}
