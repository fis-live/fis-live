{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.github-deploy.js --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint --type-check -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/effects": "^4.0.0",
    "@ngrx/store": "^4.0.0",
    "bootstrap": "4.0.0-alpha.5",
    "clarity-icons": "^0.10.0",
    "clarity-ui": "^0.10.0",
    "core-js": "^2.4.1",
    "ngrx-store-localstorage": "^0.2.0",
    "perfect-scrollbar": "^1.0.1",
    "reselect": "^3.0.0",
    "rxjs": "^5.0.1",
    "semantic-ui-css": "^2.2.4",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular-cli/base-href-webpack": "^1.0.10",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@ngtools/webpack": "^1.3.0",
    "@types/node": "^8.0.33",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.0.0",
    "codelyzer": "^3.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.0.0",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.0",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.19.0",
    "tslint": "^5.2.0",
    "typescript": "~2.4.0",
    "webpack": "^3.5.0",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^4.0.0"
  }
}
