{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.github-deploy.js --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^5.1.2",
    "@angular/common": "^5.1.2",
    "@angular/compiler": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@clr/icons": "^0.11.0",
    "@clr/ui": "^0.11.0",
    "@ngrx/effects": "^5.0.0",
    "@ngrx/entity": "^5.0.0",
    "@ngrx/store": "^5.0.0",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.5.3",
    "ngrx-store-localstorage": "^0.3.0",
    "perfect-scrollbar": "^1.3.0",
    "rxjs": "^5.5.6",
    "semantic-ui-css": "^2.2.4",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-cli/base-href-webpack": "^1.0.10",
    "@angular-devkit/build-optimizer": "^0.2.0",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/platform-server": "^5.1.2",
    "@ngtools/webpack": "^1.9.3",
    "@types/node": "^8.5.2",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^7.2.3",
    "awesome-typescript-loader": "^3.4.1",
    "codelyzer": "^4.0.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.22.0",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rxjs-tslint-rules": "^3.3.0",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.20.0",
    "tslint": "^5.2.0",
    "typescript": "~2.6.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
