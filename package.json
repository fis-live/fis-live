{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js --env.mode=dev --progress",
    "build": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path /fis-live/dist/ --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path / --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "@clr/icons": "^0.12.5",
    "@clr/ui": "^0.12.5",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.5.7",
    "ngrx-store-localstorage": "^5.0.1",
    "perfect-scrollbar": "^1.4.0",
    "rxjs": "^6.2.2",
    "semantic-ui-css": "^2.3.3",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.6.8",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@angular/platform-server": "^6.0.9",
    "@ngtools/webpack": "^6.0.8",
    "@types/node": "^8.10.21",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^8.6.5",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^4.4.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.3",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.0.6",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "postcss-loader": "^2.1.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.11.0",
    "typescript": "~2.7.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
