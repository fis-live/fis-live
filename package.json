{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.github-deploy.js --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^5.0.2",
    "@angular/common": "^5.0.2",
    "@angular/compiler": "^5.0.2",
    "@angular/core": "^5.0.2",
    "@angular/forms": "^5.0.2",
    "@angular/platform-browser": "^5.0.2",
    "@angular/platform-browser-dynamic": "^5.0.2",
    "@angular/router": "^5.0.2",
    "@ngrx/effects": "^4.1.1",
    "@ngrx/entity": "^4.1.1",
    "@ngrx/store": "^4.1.1",
    "bootstrap": "4.0.0-alpha.5",
    "clarity-icons": "^0.10.14",
    "clarity-ui": "^0.10.14",
    "core-js": "^2.4.1",
    "ngrx-store-localstorage": "^0.2.3",
    "perfect-scrollbar": "^1.2.0",
    "rxjs": "^5.5.0",
    "semantic-ui-css": "^2.2.4",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular-cli/base-href-webpack": "^1.0.10",
    "@angular-devkit/build-optimizer": "^0.0.33",
    "@angular/compiler-cli": "^5.0.2",
    "@angular/platform-server": "^5.0.2",
    "@ngtools/webpack": "^1.8.3",
    "@types/node": "^8.0.53",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^7.1.5",
    "awesome-typescript-loader": "^3.4.0",
    "codelyzer": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.0.0",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.22.0",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rxjs-tslint-rules": "^3.1.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.19.0",
    "tslint": "^5.2.0",
    "typescript": "~2.4.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  }
}
