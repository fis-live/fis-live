{
  "name": "fis-live",
  "version": "1.0.0",
  "author": "joow",
  "license": "MIT",
  "description": "Fis live app, built on Angular 2",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.js --env.mode=dev --progress",
    "build": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path /fis-live/dist/ --progress --profile --bail",
    "deploy": "rimraf dist && webpack --config config/webpack.config.js --env.mode=prod  --output-public-path / --progress --profile --bail && npm run publish",
    "publish": "gh-pages -d dist -b master -o pages",
    "lint": "tslint -p tsconfig.json",
    "build-icons": "node config/build.icons.js"
  },
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@clr/icons": "^2.1.1",
    "@clr/ui": "^2.1.1",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/entity": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "core-js": "^2.6.9",
    "ngrx-store-localstorage": "^8.0.0",
    "perfect-scrollbar": "^1.4.0",
    "rxjs": "^6.5.0",
    "semantic-ui-css": "^2.4.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.802.1",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@angular/platform-server": "^8.2.2",
    "@ngtools/webpack": "^8.2.1",
    "@types/node": "^12.7.1",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^9.6.1",
    "awesome-typescript-loader": "^5.2.0",
    "codelyzer": "^5.1.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "tslint": "^5.18.0",
    "typescript": "~3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
